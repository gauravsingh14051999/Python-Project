x=int(input("Enter Number "))
l=[]
if x>1:
  for j in range(2,x):
      for i in range(2,j):
          if j%i==0:
            #print("Not prime no",x)
            break
      else:
          l.append(j)
print("The prime number between 1 and 50 are ")
for k in l:
   print(k,end=" ")
     
###########################################
#Print n prime number

#prime number
for i in range(2,50):
  for j in range(2,i):
    if i%j==0:
       break
  else:
    print(i,end=" ")
